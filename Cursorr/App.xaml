<Application x:Class="Cursorr.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Cursorr"
             StartupUri="UI/Pages/MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Culture-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Strings.xaml"/>
                <!--Values-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Values.xaml"/>
                <!--Theme-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme.xaml"/>
                <!--Assets-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Assets.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
